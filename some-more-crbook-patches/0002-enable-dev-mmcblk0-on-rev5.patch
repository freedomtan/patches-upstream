From: Koan-Sin Tan <freedom.tan@mediatek.com>
Date: Fri, 3 Jun 2016 17:08:16 +0800
Subject: [PATCH 2/3] enable /dev/mmcblk0* on rev5

---
 arch/arm64/boot/dts/mediatek/mt8173-oak.dtsi | 4 ++++
 1 file changed, 4 insertions(+)

diff --git a/arch/arm64/boot/dts/mediatek/mt8173-oak.dtsi b/arch/arm64/boot/dts/mediatek/mt8173-oak.dtsi
index 071f7c3..dec0e9d 100644
--- a/arch/arm64/boot/dts/mediatek/mt8173-oak.dtsi
+++ b/arch/arm64/boot/dts/mediatek/mt8173-oak.dtsi
@@ -310,10 +310,14 @@
 	bus-width = <8>;
 	max-frequency = <200000000>;
 	cap-mmc-highspeed;
+/*
 	mmc-hs200-1_8v;
 	mmc-hs400-1_8v;
+*/
 	cap-mmc-hw-reset;
+/*
 	hs400-ds-delay = <0x18009>;
+*/
 	vmmc-supply = <&mt6397_vemc_3v3_reg>;
 	vqmmc-supply = <&mt6397_vio18_reg>;
 	assigned-clocks = <&topckgen CLK_TOP_MSDC50_0_SEL>;
