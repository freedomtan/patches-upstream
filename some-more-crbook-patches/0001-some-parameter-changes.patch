Subject: [PATCH 1/2] some parameter changes

---
 arch/arm64/boot/dts/mediatek/mt8173-oak.dtsi |  4 ++--
 arch/arm64/boot/dts/mediatek/mt8173.dtsi     | 19 ++++++++++---------
 2 files changed, 12 insertions(+), 11 deletions(-)

diff --git a/arch/arm64/boot/dts/mediatek/mt8173-oak.dtsi b/arch/arm64/boot/dts/mediatek/mt8173-oak.dtsi
index 0f29e66..071f7c3 100644
--- a/arch/arm64/boot/dts/mediatek/mt8173-oak.dtsi
+++ b/arch/arm64/boot/dts/mediatek/mt8173-oak.dtsi
@@ -860,8 +860,8 @@
 			mt6397_vgp3_reg: ldo_vgp3 {
 				regulator-compatible = "ldo_vgp3";
 				regulator-name = "vcamaf";
-				regulator-min-microvolt = <1620000>;
-				regulator-max-microvolt = <1980000>;
+				regulator-min-microvolt = <1200000>;
+				regulator-max-microvolt = <3300000>;
 				regulator-enable-ramp-delay = <218>;
 			};
 
diff --git a/arch/arm64/boot/dts/mediatek/mt8173.dtsi b/arch/arm64/boot/dts/mediatek/mt8173.dtsi
index e9f62db..080fd1a 100644
--- a/arch/arm64/boot/dts/mediatek/mt8173.dtsi
+++ b/arch/arm64/boot/dts/mediatek/mt8173.dtsi
@@ -224,9 +224,9 @@
 			CPU_SLEEP_0: cpu-sleep-0 {
 				compatible = "arm,idle-state";
 				local-timer-stop;
-				entry-latency-us = <72>;
-				exit-latency-us = <66>;
-				min-residency-us = <458>;
+				entry-latency-us = <100>;
+				exit-latency-us = <800>;
+				min-residency-us = <2000>;
 				arm,psci-suspend-param = <0x40000002>;
 			};
 		};
@@ -247,9 +247,10 @@
 	pd-power-states {
 		CLUSTER0_RET: power-state1@1 {
 			state-param = <0x40000022>;
-			entry-latency-us = <145>;
-			exit-latency-us = <60>;
-			residency-us = <331>;
+			entry-latency-us = <350>;
+			exit-latency-us = <80>;
+			residency-us = <3000>;
+
 		};
 
 		CLUSTER0_PWR_DWN: power-state1@2 {
@@ -261,9 +262,9 @@
 
 		CLUSTER1_RET: power-state2@1 {
 			state-param = <0x40000022>;
-			entry-latency-us = <287>;
-			exit-latency-us = <62>;
-			residency-us = <1423>;
+			entry-latency-us = <350>;
+			exit-latency-us = <80>;
+			residency-us = <3000>;
 		};
 
 		CLUSTER1_PWR_DWN: power-state2@2 {
